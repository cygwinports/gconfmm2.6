libgconfmm2_6_devel_CONTENTS='usr/include/ usr/lib/'
libgconfmm2_6_doc_CATEGORY="Doc"
libgconfmm2_6_doc_CONTENTS="usr/share/devhelp/ usr/share/doc/gconfmm-2.6/"
